{
    "openapi": "3.0.0",
    "info": {
        "title": "Aloka E - Channeling And Pharmacy Management System",
        "description": "Backend API",
        "contact": {
            "name": "Kulesha Rashmini",
            "email": "kulesharashmini@gmail.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "/api/v1",
            "description": "Development Server"
        }
    ],
    "components": {
        "responses": {
            "errorResponse": {
                "description": "Return error message",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "unauthorizedResponse": {
                "description": "Return login required error message",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "default": "Log in required"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "invalidatedResponse": {
                "description": "Return validation errors with error message",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "notFoundResponse": {
                "description": "Return item not found error message",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "createdResponse": {
                "description": "Return created item with confirmation message",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "updatedResponse": {
                "description": "Return updated item with confirmation message",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "deletedResponse": {
                "description": "Return delete confirmation message",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "itemFoundResponse": {
                "description": "Return item with confirmation message",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "listFoundResponse": {
                "description": "Return items list with confirmation message",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}